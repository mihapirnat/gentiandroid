<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true" >
	
        <shape>
            <solid
                android:color="#ffa24a" />
            
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
    <item>
    
        <shape>
            <gradient
                android:startColor="#eff3ef"
                android:endColor="#c6c3c6"
                android:angle="270" />
            
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item></selector>
